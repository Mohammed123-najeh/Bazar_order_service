{
    "name": "bazar/order-service",
    "description": "Order service for Bazar microservices",
    "keywords": ["lumen", "microservice", "order"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "laravel/lumen-framework": "^10.0",
        "guzzlehttp/guzzle": "^7.5",
        "nesbot/carbon": "^2.68"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
